generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id                           Int       @id @default(autoincrement())
  email                        String?
  fdai_user_id                 Int?
  first_name                   String?
  username                     String?
  fdai_access_token            String?
  fdai_refresh_token           String?
  fdai_expires_in              Int?
  fdai_scope                   String?
  fdai_access_token_expires_at DateTime?
}
