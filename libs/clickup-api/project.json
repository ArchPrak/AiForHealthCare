{
  "name": "clickup-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clickup-api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/clickup-api",
        "main": "libs/clickup-api/src/index.ts",
        "tsConfig": "libs/clickup-api/tsconfig.lib.json",
        "assets": ["libs/clickup-api/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/clickup-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/clickup-api"],
      "options": {
        "jestConfig": "libs/clickup-api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["api"]
}
